{
  "name": "vault-ui",
  "private": true,
  "version": "0.0.1",
  "description": "UI for interacting with Vault",
  "repository": "https://github.com/gambtho/vault-portal/",
  "license": "Apache2",
  "devDependencies": {
    "html-loader": "^0.4.3",
    "jasmine-core": "^2.3.4",
    "karma": "~0.13",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.5",
    "karma-junit-reporter": "^0.4.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.4",
    "karma-webpack": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.4",
    "protractor": "^3.1.1",
    "rimraf": "^2.5.2",
    "shelljs": "^0.6.0",
    "sinon": "^1.17.3",
    "ts-loader": "^0.8.1",
    "typescript": "^1.8.7",
    "typings": "^0.7.8",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "awesome-typescript-loader": "0.16.0-rc.0",
    "ie-shim": "^0.1.0",
    "source-map-loader": "^0.1.5",
    "webpack-merge": "^0.8.3"
  },
  "scripts": {
    "pretest": "npm install",
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "watch": "webpack --watch --progress --profile",
    "build": "rimraf dist && webpack",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 8081 --content-base app",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "ci": "npm run test && npm run e2e",
    "start": "npm run server",
    "postinstall": "npm run webdriver-update && typings install",
    "prestart": "npm install",
    "test-watch": "karma start test/karma.conf.js",
    "test": "npm run build && karma start test/karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor test/protractor-conf.js"
  },
  "dependencies": {
    "angular2": "^2.0.0-beta.9",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.33.13",
    "reflect-metadata": "0.1.2",
    "rxjs": "^5.0.0-beta.2",
    "zone.js": "^0.5.15"
  }
}
